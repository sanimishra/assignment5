{
    "extends": "airbnb-base",
    "env": {
      "node": true
    },
    "rules": {
      "import/extensions": [ "error", "always", { "ignorePackages": true } ],
      "no-console": "off",
      "jsx-a11y/label-has-for": [ 2, {
        "components": [ "Label" ],
        "required": {
            "some": [ "nesting", "id" ]
        },
        "allowChildren": false
    }]
    }
  }