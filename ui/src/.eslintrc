{
    "extends": "airbnb",
    "env": {
        "browser": true
    },
    "rules": {
        "react/prop-types": "off",
        "jsx-a11y/label-has-for": [ 2, {
            "components": [ "Label" ],
            "required": {
                "some": [ "nesting", "id" ]
            },
            "allowChildren": false,
        }]
    }
}